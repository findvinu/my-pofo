  <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
  <div
    class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
  >
    <h1 class="h2">Add New Projects</h1>
    <div class="btn-toolbar mb-2 mb-md-0">
   
    </div>
  </div>
  <form action="#" method="POST">
    <div class="row">
      <div class="col-md-6">
        <div class="mb-3">
          <label for="name" class="form-label">Name</label>
          <input
            type="text"
            class="form-control"
            id="name"
           placeholder="Name"
            name="name"
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="mb-3">
          <label for="alias" class="form-label">Alias</label>
          <input
            type="text"
            class="form-control"
            id="alias"
            placeholder="Alias"
            name="alias"
            readonly
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="mb-3">
          <label for="github" class="form-label">Github URL</label>
          <input
            type="text"
            class="form-control"
            id="github"
             placeholder="Github"
            name="github"
          />
        </div>
      </div>

      <div class="col-md-6">
        <div class="mb-3">
          <label for="tags" class="form-label">Tags</label>
          <input
            type="text"
            class="form-control"
            id="tags"
            placeholder="Tags"
            name="tags"
          />
        </div>
      </div>
      <div class="col-md-6">
        <div class="mb-3">
          <textarea
            class="form-control"
            name="description"
            name="description"
            id="desc"
            rows="3"
          >Description</textarea>
        </div>
      </div>
        <div class="col-md-6">
        <div class="mb-3">
         <input type="image" />
        </div>
      </div>
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
  </form>
  </main>