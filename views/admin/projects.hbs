<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
  <div
    class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
  >
    <h1 class="h2">Projects</h1>
    <div class="btn-toolbar mb-2 mb-md-0">
      <div class="btn-group me-2">
        <a href="/admin/newProject" class="btn btn-sm btn-outline-secondary">Create
          New</a>
        <button
          type="button"
          class="btn btn-sm btn-outline-secondary"
        >Upload</button>
      </div>
    </div>
  </div>

  <div class="table-responsive">
    <table class="table table-striped table-sm">
      <thead>
        <tr>
          <th scope="col">Image</th>
          <th scope="col">Name</th>
          <th scope="col">Alias</th>
          <th scope="col">Github</th>
          <th scope="col">Action</th>
        </tr>
      </thead>
      <tbody>
        {{#each projects}}
          <tr>
            <td><img src="/images/mean.jfif" alt="mean" height="50px" /></td>
            <td><a href="/admin/projects/{{this.alias}}">{{this.name}}</a></td>
            <td><a href="/admin/projects/{{this.alias}}">{{this.alias}}</a></td>
            <td><a href="{{this.githubUrl}}">Link</a></td>
            <td>

            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</main>